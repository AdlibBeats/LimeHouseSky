<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <FontFamily x:Key="SFNSDisplay">sfns-display-thin.ttf#SFNS Display</FontFamily>
            <FontFamily x:Key="HelveticaNeueCyr">helveticaneuecyr-ultralight3.ttf#HelveticaNeueCyr</FontFamily>
            <FontFamily x:Key="OpenSans">open-sans.ttf#Open Sans</FontFamily>
            <FontFamily x:Key="CaviarDreams">caviar-dreams.ttf#Caviar Dreams</FontFamily>

            <FontFamily x:Key="ContentControlThemeFontFamily">M.video_regular.otf#M.Video</FontFamily>
            <FontFamily x:Key="MvideoFontRegular">M.video_regular.otf#M.Video</FontFamily>
            <FontFamily x:Key="MvideoFontBold">M.video_bold.otf#M.Video</FontFamily>
            <FontFamily x:Key="MvideoFontBoldCond">M.video_bold_cond.otf#M.Video</FontFamily>
            <FontFamily x:Key="MvideoFontLight">M.video_light.otf#M.Video</FontFamily>
            <FontFamily x:Key="MvideoIcon">MVideo_icon_font.ttf#MVideo_icon_font</FontFamily>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
    
</ResourceDictionary>
